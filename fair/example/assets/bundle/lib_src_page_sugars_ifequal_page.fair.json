{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "%(getTitle)"
          ]
        }
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "Column",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.center)",
            "children": [
              {
                "className": "Card",
                "na": {
                  "clipBehavior": "#(Clip.hardEdge)",
                  "child": {
                    "className": "Padding",
                    "na": {
                      "padding": {
                        "className": "EdgeInsets.only",
                        "na": {
                          "top": 20
                        }
                      },
                      "child": {
                        "className": "Text",
                        "pa": [
                          "#(_count = $_count)"
                        ]
                      }
                    }
                  }
                }
              },
              {
                "className": "Padding",
                "na": {
                  "padding": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "top": 20
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "_count == 2,  update image !"
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "floatingActionButton": {
      "className": "Row",
      "na": {
        "mainAxisAlignment": "#(MainAxisAlignment.end)",
        "children": [
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.threesixty)"
                ]
              },
              "onPressed": "@(onTapText)"
            }
          }
        ]
      }
    }
  },
  "methodMap": {
    "createState": "%(_State)"
  }
}